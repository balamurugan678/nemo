<?xml version="1.0"?>
<mapping>

    <description>Object/XML mapping for Nemo TfL Common Pre Pay Ticket</description>
    <class name="com.novacroft.nemo.tfl.common.domain.cubic.CardUpdatePrePayTicketRequest" >
        <map-to xml="CardUpdateRequest" />
        <field name="realTimeFlag">
            <bind-xml name="RealTimeFlag" node="element"/>
        </field>
        <field name="prestigeId">
            <bind-xml name="PrestigeID" node="element"/>
        </field>
        <field name="action">
            <bind-xml name="Action" node="element"/>
        </field>
        <field name="productCode">
            <bind-xml name="ProductCode" node="element" location="PPT"/>
        </field>
        <field name="startDate">
            <bind-xml name="StartDate" node="element" location="PPT" />
        </field>
        <field name="expiryDate">
            <bind-xml name="ExpiryDate" node="element" location="PPT"/>
        </field>
        <field name="productPrice">
            <bind-xml name="ProductPrice" node="element" location="PPT"/>
        </field>
        <field name="currency">
            <bind-xml name="Currency" node="element" location="PPT" />
        </field>
        <field name="paymentMethod">
            <bind-xml name="PaymentMethod" node="element"/>
        </field>
        <field name="pickupLocation">
            <bind-xml name="PickupLocation" node="element"/>
        </field>
        <field name="userId">
            <bind-xml name="UserID" node="element" location="OriginatorInfo" />
        </field>
        <field name="password">
            <bind-xml name="Password" node="element" location="OriginatorInfo"/>
        </field>
    </class> 
</mapping>
